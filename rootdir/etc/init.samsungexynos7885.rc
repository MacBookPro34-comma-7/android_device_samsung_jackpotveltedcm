    mkdir /data/misc/wifi 0770 wifi system
    mkdir /data/misc/wifi/sockets 0770 wifi wifi
    mkdir /vendor/etc/wifi 0770 system system
    mkdir /data/hostapd 0770 system wifi
	
# Permissions for audio
